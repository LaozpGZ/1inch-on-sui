# Production Environment Configuration for 1inch-on-Sui Hackathon
# 
# IMPORTANT: Never commit actual API keys to version control.
# This file shows the required variables for production deployment.

# Production Environment Flag
NEXT_PUBLIC_ENVIRONMENT=production
NEXT_PUBLIC_DEBUG=false

# Demo Mode Configuration
NEXT_PUBLIC_DEMO_MODE=true
NEXT_PUBLIC_ENABLE_MOCK_SERVICES=true

# Ethereum Configuration (Sepolia Testnet for Hackathon)
NEXT_PUBLIC_ALCHEMY_API_KEY=${ALCHEMY_API_KEY}
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=${WALLET_CONNECT_PROJECT_ID}
NEXT_PUBLIC_RPC_URL_SHAPE_SEPOLIA=${SHAPE_SEPOLIA_RPC_URL}
NEXT_PUBLIC_CHAIN_ID=11011

# 1inch Fusion API Configuration  
NEXT_PUBLIC_1INCH_AUTH_KEY=${ONEINCH_AUTH_KEY}

# Relayer Service Configuration (Production URLs)
NEXT_PUBLIC_RELAYER_API_URL=${RELAYER_API_URL}
NEXT_PUBLIC_RELAYER_WS_URL=${RELAYER_WS_URL}

# Performance Configuration
NEXT_PUBLIC_POLLING_INTERVAL=30000
NEXT_PUBLIC_REQUEST_TIMEOUT=10000
NEXT_PUBLIC_MAX_RETRIES=3

# Analytics and Monitoring (Optional for hackathon)
NEXT_PUBLIC_ANALYTICS_ENABLED=false
NEXT_PUBLIC_ERROR_REPORTING_ENABLED=false

# Build Configuration
NEXT_PUBLIC_IGNORE_BUILD_ERROR=false
VERCEL_TELEMETRY_DISABLED=1